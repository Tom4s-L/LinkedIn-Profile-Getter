{
  "manifest_version": 3,
  "name": "LinkedIn Profile Getter",
  "version": "0.0.1",
  "description": "An extension to get the profile of a LinkedIn user",
  "action": {
    "default_title": "Open the app"
  },
  "background": {
    "service_worker": "dist/background/index.mjs"
  },
  "content_scripts": [
    {
      "matches": ["*://*.linkedin.com/*"],
      "js": ["dist/content/index.mjs"]
    }
  ],
  "host_permissions": [
    "https://api.notion.com/*"
  ]
}
