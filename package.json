{
  "name": "test-webext",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@9.12.2",
  "description": "An extension to get the profile of a LinkedIn user",
  "scripts": {
    "dev": "vite",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "run-s clear build:prepare build:web build:background",
    "build:prepare": "esno scripts/prepare.ts",
    "build:web": "vite build",
    "build:background": "vite build --config vite.config.background.mts",
    "clear": "rimraf extension"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.14.1",
    "@types/chrome": "^0.0.326",
    "@types/firefox-webext-browser": "^120.0.4",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.15.30",
    "@types/webextension-polyfill": "^0.12.3",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@unocss/reset": "^66.1.4",
    "@vitejs/plugin-vue": "^5.2.4",
    "eslint": "^9.28.0",
    "esno": "^4.8.0",
    "fs-extra": "^11.3.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "unocss": "^66.1.4",
    "vite": "^6.3.5",
    "vue": "^3.5.16",
    "webextension-polyfill": "^0.12.0"
  }
}
